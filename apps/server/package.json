		"prisma": "^4.3.1",
		"supertest": "^6.2.4",
		"x-xss-protection": "^2.0.0"
	},
	"scripts": {
		"dev": "tsnd -r tsconfig-paths/register lib/server.ts",
		"start": "node dist/lib/server.js",
		"build": "tsc --outDir dist && cp lib/config/swagger.yaml dist/lib/config/",
		"lint": "eslint . --ext .ts",
		"fix": "eslint 'lib/**/*.ts' --fix",
		"format": "prettier --write 'lib/**/*.ts'",
		"test": "jest --maxWorkers=1 --bail --ci --coverage",
		"test:ci": "jest --ci --coverage",
		"test:watch": "jest --watch --forceExit --detectOpenHandles --maxWorkers=1",
		"migrate": "prisma migrate dev --name init"
	}
