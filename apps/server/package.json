		"@fastify/swagger": "^7.5.1",
		"@prisma/client": "^4.3.1",
		"bcrypt": "^5.0.1",
		"dotenv": "^16.0.2",
		"fastify": "^4.6.0",
		"frameguard": "^4.0.0",
